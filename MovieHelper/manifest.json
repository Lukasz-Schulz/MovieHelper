{
    "manifest_version": 2,
    "name": "MovieHelper",
    "version": "1.0",
    "description": "A movie-choosing extention",
    "icons": {
        "128":"128.png",
        "48":"48.png",
        "16":"16.png"
    },
    "browser_action":{
        "default_icon": "48.png",
        "default_popup": "popup.html"
    },
    
    "options_page": "options.html",

    "permissions": [
        "storage"
    ]
}